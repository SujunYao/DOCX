{
    "compilerOptions": {
        "module": "commonjs",
        "baseUrl": "./",
        "outDir": "dist",
        "target": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "lib": [
            "es6",
            "dom"
        ],
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "jsx": "react"
    },
    "include": [
        "**.ts", "api/server.ts"
    ]
}